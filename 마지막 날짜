#include <stdio.h>
#include <stdlib.h>
#define SIZE 12
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int days[SIZE] = {31,29,31,30,31,30,31,31,30,31,30,31};
	int i;
	
	for(i=1;i<13;i++){
		printf("%d월은 %d일 까지 입니다.\n", i, days[i-1]);
	}
	return 0;
}
