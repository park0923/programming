#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int num[3][10] ={0,};
	int x, i, j;
	
	
	for(i=0;i<10;i++){
		num[0][i] = i + 1;
		printf(" %3d", num[0][i]);
	}
	printf("\n");
	
	for(i=0;i<10;i++){
		num[1][i] = num[0][i] * num[0][i];
		printf(" %3d", num[1][i]);
	}
	printf("\n");
	
	for(i=0;i<10;i++){
		num[2][i] = num[1][i] * num[0][i];
		printf(" %3d", num[2][i]);
	}
	printf("\n");
	
	printf("정수를 입력하시오.:");
	scanf("%d", &x); 
	
	for(i=0;i<10;i++){
		for(j=0;j<10;j++){
			if(num[2][j] == x){
				printf("%d 의 세제곱근은 %d 입니다.", x, num[0][j]);
				return 0;
			}
		}
		for(j=0;j<10;j++){
			if(num[2][j] != x){
				printf("erro");
				return 0;
			}
		}
	}
	return 0;
}
